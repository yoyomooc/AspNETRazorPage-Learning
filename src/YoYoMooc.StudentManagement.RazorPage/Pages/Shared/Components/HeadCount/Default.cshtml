
@model IEnumerable<MajorHeadCount>

<h3>学生人数汇总</h3>

<table class="table table-bordered">
    <thead class="thead-light">
        <tr>
            <th>专业</th>
            <th>合计人数</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Major</td>
                <td>@item.Count</td>
            </tr>
        }
    </tbody>
</table>